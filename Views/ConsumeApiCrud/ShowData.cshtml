@model IEnumerable<ConsumeWebApi.Models.StudentsData>
@{
    ViewData["Title"] = "ShowData";
}
<html>
    <head>
        <title> Dashboard</title>
        <style>
            table{
                padding:5px;
                border:1px solid red;
            }
        </style>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    </head>
    <body>
    @Html.ActionLink("Create New","Create","ConsumeApiCrud")

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>recordId</th>
                <th>RollNo</th>
                <th>Name</th>
                <th>EmailID</th>
                <th>Password</th>
                <th>Age</th>
                <th>Dob</th>
                <th>fee</th>
                <th>Dept</th>
                <th> status</th>
                <th> role</th>
                <th> Action </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var i in Model)
            {
                <tr>
                    <td> @i.Id </td>
                    <td> @i.recordId </td>
                    <td> @i.RollNo </td>
                    <td> @i.Name </td>
                    <td> @i.EmailID </td>
                    <td> @i.Password </td>
                    <td> @i.Age </td>
                    <td> @i.Dob </td>
                    <td> @i.fee </td>
                    <td> @i.Dept </td>
                    <td> @i.status </td>
                    <td> @i.role </td>
                    <td>

                        @Html.ActionLink("Detials", "Detials", "ConsumeApiCrud", new{Id = i.Id}) |
                        @Html.ActionLink("Edit", "Edit", "ConsumeApiCrud", new{Id = i.Id}) |
                        @Html.ActionLink("Delete", "Delete", "ConsumeApiCrud", new{Id = i.Id}) 

                    </td>
                </tr>
            }
        </tbody>
    </table>
    </body>
</html>